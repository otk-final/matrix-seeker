{
  "name": "详情页",
  "bind": {
    "position": ".wenCon",
    "fields": [
      {
        "selector": ".article_hd",
        "findType": "text",
        "mapper": "title"
      },
      {
        "selector": ".wenConDes",
        "findType": "text",
        "mapper": "content"
      },
      {
        "selector": ".wenConDes img",
        "findType": "attr:src",
        "valueType": "[]",
        "mapper": "imgArray"
      }
    ]
  }
}